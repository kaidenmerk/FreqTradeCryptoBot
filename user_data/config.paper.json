{
  "trading_mode": "spot",
  "margin_mode": "",
  "max_open_trades": 8,
  "stake_currency": "USD",
  "stake_amount": "unlimited",
  "tradable_balance_ratio": 0.99,
  "fiat_display_currency": "USD",

  "dry_run": false,
  "cancel_open_orders_on_exit": true,
  
  "ignore_outdated_data": true,

  "unfilledtimeout": {
    "entry": 30,
    "exit": 30,
    "exit_timeout_count": 0,
    "unit": "minutes"
  },

  "entry_pricing": {
    "price_side": "same",
    "use_order_book": false,
    "order_book_top": 1,
    "price_last_balance": 0.0,
    "check_depth_of_market": {
      "enabled": false,
      "bids_to_ask_delta": 1
    }
  },

  "exit_pricing": {
    "price_side": "same",
    "use_order_book": false,
    "order_book_top": 1
  },

  "exchange": {
    "name": "alpaca",
    "key": "PKZPW4OGB8L48YXQHI4A",
    "secret": "zIsxFopw8rYeOmrGmX456W53goaHXbyc5LQ5rZRl",
    "ccxt_config": {
      "enableRateLimit": true,
      "rateLimit": 500,
      "sandbox": true,
      "urls": {
        "api": "https://paper-api.alpaca.markets"
      }
    },
    "pair_whitelist": [
      "BTC/USD", "ETH/USD", "ADA/USD", "AVAX/USD", "BAT/USD", "BCH/USD", 
      "COMP/USD", "CRV/USD", "DOGE/USD", "DOT/USD", "ETC/USD", "FIL/USD",
      "GRT/USD", "ICP/USD", "LINK/USD", "LTC/USD", "MATIC/USD", "MKR/USD",
      "NEAR/USD", "SHIB/USD", "SOL/USD", "SUSHI/USD", "UNI/USD", "XLM/USD",
      "XTZ/USD", "YFI/USD", "AAVE/USD", "ALGO/USD", "ATOM/USD"
    ],
    "pair_blacklist": []
  },

  "dataexchange": {
    "name": "coinbase",
    "ccxt_config": {
      "enableRateLimit": true,
      "rateLimit": 100
    }
  },

  "pairlists": [
    {
      "method": "StaticPairList"
    }
  ],

  "edge": {
    "enabled": false
  },

  "strategy": "DonchianATRTrend",
  "strategy_path": "user_data/strategies/",
  "timeframe": "5m",

  "process_only_new_candles": true,
  "minimal_roi": {
    "0": 0.04,
    "120": 0.02,
    "240": 0.01,
    "360": 0.0
  },
  "stoploss": -0.05,
  "trailing_stop": true,
  "trailing_stop_positive": 0.02,
  "trailing_stop_positive_offset": 0.03,
  "trailing_only_offset_is_reached": true,

  "ignore_roi_if_entry_signal": false,
  "ignore_buying_expired_candle_after": 600,

  "dataformat_ohlcv": "json",
  "dataformat_trades": "jsongz",

  "position_adjustment_enable": false,
  "max_entry_position_adjustment": -1,

  "order_types": {
    "entry": "limit",
    "exit": "limit",
    "emergency_exit": "market",
    "force_exit": "market",
    "force_entry": "market",
    "stoploss": "market",
    "stoploss_on_exchange": false,
    "stoploss_on_exchange_interval": 60
  },

  "order_time_in_force": {
    "entry": "GTC",
    "exit": "GTC"
  },

  "plot_config": {
    "main_plot": {
      "don_upper_entry": {"color": "red"},
      "don_lower_entry": {"color": "red"},
      "don_mid_exit": {"color": "blue"},
      "ema": {"color": "orange"}
    },
    "subplots": {
      "ATR": {
        "atr": {"color": "purple"}
      },
      "RSI": {
        "rsi": {"color": "green"}
      }
    }
  },

  "internals": {
    "process_throttle_secs": 5
  },

  "db_url": "sqlite:///user_data/trades.alpaca.paper.sqlite",
  "initial_state": "running",
  "force_entry_enable": false,

  "discord": {
    "enabled": true,
    "webhook_url": "${DISCORD_WEBHOOK_URL}",
    "entry": "on",
    "entry_fill": "on", 
    "exit": "on",
    "exit_fill": "on",
    "status": "on",
    "startup": "on",
    "buy_tag": "on",
    "sell_reason": "on"
  },

  "api_server": {
    "enabled": true,
    "listen_ip_address": "127.0.0.1",
    "listen_port": 8080,
    "verbosity": "error",
    "enable_openapi": false,
    "jwt_secret_key": "somethingrandom",
    "CORS_origins": [],
    "username": "freqtrade",
    "password": "freqtrade"
  },

  "bot_name": "freqtrade-alpaca-paper",
  "user_data_dir": "user_data",
  "datadir": "user_data/data/coinbase"
}
