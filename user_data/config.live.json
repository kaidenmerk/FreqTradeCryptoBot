{
  "trading_mode": "spot",
  "margin_mode": "",
  "max_open_trades": 2,
  "stake_currency": "USD",
  "stake_amount": 25,
  "tradable_balance_ratio": 0.99,
  "fiat_display_currency": "USD",

  "dry_run": false,
  "cancel_open_orders_on_exit": true,

  "unfilledtimeout": {
    "entry": 60,
    "exit": 60,
    "exit_timeout_count": 0,
    "unit": "minutes"
  },

  "entry_pricing": {
    "price_side": "same",
    "use_order_book": true,
    "order_book_top": 1,
    "price_last_balance": 0.0,
    "check_depth_of_market": {
      "enabled": false,
      "bids_to_ask_delta": 1
    }
  },

  "exit_pricing": {
    "price_side": "same",
    "use_order_book": true,
    "order_book_top": 1
  },

  "exchange": {
    "name": "coinbase",
    "key": "${CB_KEY}",
    "secret": "${CB_SECRET}",
    "password": "${CB_PASSPHRASE}",
    "ccxt_config": {
      "enableRateLimit": true,
      "rateLimit": 100
    },
    "pair_whitelist": [
      "BTC/USD",
      "ETH/USD", 
      "SOL/USD",
      "MATIC/USD",
      "AVAX/USD"
    ],
    "pair_blacklist": []
  },

  "pairlists": [
    {
      "method": "StaticPairList"
    }
  ],

  "edge": {
    "enabled": false
  },

  "protections": [
    {
      "method": "StoplossGuard",
      "lookback_period_candles": 24,
      "trade_limit": 2,
      "stop_duration_candles": 24,
      "only_per_pair": false
    },
    {
      "method": "MaxDrawdown",
      "lookback_period_candles": 168,
      "trade_limit": 2,
      "max_allowed_drawdown": 0.05,
      "stop_duration_candles": 72
    },
    {
      "method": "CooldownPeriod",
      "stop_duration_candles": 6,
      "only_per_pair": true
    }
  ],

  "strategy": "DonchianATRTrend",
  "strategy_path": "user_data/strategies/",
  "timeframe": "1h",
  "timerange": "",

  "process_only_new_candles": true,
  "minimal_roi": {
    "0": 0.03
  },
  "stoploss": -0.05,
  "trailing_stop": false,

  "ignore_roi_if_entry_signal": false,
  "ignore_buying_expired_candle_after": 300,

  "dataformat_ohlcv": "json",
  "dataformat_trades": "jsongz",

  "position_adjustment_enable": false,
  "max_entry_position_adjustment": -1,

  "order_types": {
    "entry": "limit",
    "exit": "limit",
    "emergency_exit": "market",
    "force_exit": "market",
    "force_entry": "market",
    "stoploss": "market",
    "stoploss_on_exchange": false,
    "stoploss_on_exchange_interval": 60
  },

  "order_time_in_force": {
    "entry": "GTC",
    "exit": "GTC"
  },

  "plot_config": {
    "main_plot": {
      "don_upper_entry": {"color": "red"},
      "don_lower_entry": {"color": "red"},
      "don_mid_exit": {"color": "blue"},
      "ema": {"color": "orange"}
    },
    "subplots": {
      "ATR": {
        "atr": {"color": "purple"}
      },
      "RSI": {
        "rsi": {"color": "green"}
      }
    }
  },

  "internals": {
    "process_throttle_secs": 5
  },

  "db_url": "sqlite:///user_data/trades.live.sqlite",
  "initial_state": "running",
  "force_entry_enable": false,

  "api_server": {
    "enabled": true,
    "listen_ip_address": "127.0.0.1",
    "listen_port": 8081,
    "verbosity": "error",
    "enable_openapi": false,
    "jwt_secret_key": "somethingrandom-live",
    "CORS_origins": [],
    "username": "freqtrade",
    "password": "freqtrade"
  },

  "bot_name": "freqtrade-live",
  "user_data_dir": "user_data"
}
